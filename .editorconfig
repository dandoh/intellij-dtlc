[*]
charset = utf-8
end_of_line = lf
insert_final_newline = false
indent_style = tab
indent_size = tab
tab_width = 2

[*.json]
ij_json_wrap_long_lines = true
ij_json_keep_line_breaks = true

[gradle.properties]
indent_size = 0

[*.yml]
indent_size = 2
indent_style = space

[*.rst]
indent_size = 3
indent_style = space